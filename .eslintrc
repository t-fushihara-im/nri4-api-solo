{
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true,
      "mocha": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "plugins": ["prettier"],
    "rules": {
      "indent": [
        "warn",
        2
      ],
      "no-console": 0,
      "no-unused-vars": [
        "warn"
      ],
      "prettier/prettier": "error",
      "semi": [
        "error",
        "always"
      ],
      "quotes": [
        "warn"
      ]
    }
  }